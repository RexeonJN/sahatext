                        <input type="hidden" name="_csrf" value="<%= csrfToken %>">
                        <button class="btn btn-dark" type="submit">Add to Cart</button>
                        <input type="hidden" name="productId" value="<%= brand.id %>">
                    </form>

                    <% } %>
          </div>
        </div>
        <% } %>
        
      </div>
</div>




